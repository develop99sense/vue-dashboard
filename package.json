{
  "name": "vue-vuex-boilerplate",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:bamboo-c/vue-vuex-boilerplate.git",
  "author": "komei otake <komei1220@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn run clean && yarn run test && yarn run compile",
    "build:parts": "yarn run sass && yarn run compile",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && yarn run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && yarn run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "yarn run coverage:run && yarn run coverage:remap && yarn run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "@updivision/vue2-multi-uploader": "^1.6.2",
    "axios": "^0.18.0",
    "stripe": "^6.10.0",
    "vue": "^2.5.2",
    "vue-axios": "^2.1.3",
    "vue-class-component": "^6.0.0",
    "vue-cli": "^2.9.1",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-stripe-elements-plus": "^0.2.8",
    "vuejs-datepicker": "^1.5.3",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.46",
    "@types/sinon": "^2.3.6",
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.2.3",
    "bootstrap-sass": "^3.3.7",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "extract-text-webpack-plugin": "^3.0.0-rc.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.0.1",
    "karma-webpack": "^2.0.5",
    "lodash.merge": "^4.6.0",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "opn-cli": "^3.1.0",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.8",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0",
